<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="rpk_stock_tree">
    <field name="name">rpk.stock.tree</field>
    <field name="model">rpk.stock</field>
    <field name="arch" type="xml">
      <tree string="Stock">
        <field name="name"/>
        <field name="quantity"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="rpk_log_tree">
    <field name="name">rpk.log.tree</field>
    <field name="model">rpk.log</field>
    <field name="arch" type="xml">
      <tree string="Log">
        <field name="stock_id"/>
        <field name="date"/>
        <field name="description"/>
        <field name="change"/>
      </tree>
    </field>
  </record>

